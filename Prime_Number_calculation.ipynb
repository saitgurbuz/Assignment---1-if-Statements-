   "source": [
    "#### solution_1\n",
    "n = int(input(\"enter a number to check if it is a prime or not:\"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1, n+1) :\n",
    "    if not (n % i) :\n",
    "        count += 1\n",
    "\n",
    "if (n == 0) or (n == 1) or (count >=3) : \n",
    "    print(n, \"is not a prime number.\")\n",
    "else : \n",
    "    print(n, \"is a prime number\")"
   ]
